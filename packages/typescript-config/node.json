{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 22",
    "_version": "22.0.0",
  
    "compilerOptions": {
    "target": "ESNext",
    "module": "Preserve",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  }
}